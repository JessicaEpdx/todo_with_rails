<h1>Edit List: <%=@list.name%> </h1>

<%= render "layouts/errors", :object => @list %>

<%= form_for @list do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.submit %>
<% end %>

<p><%= link_to "Return to Lists", lists_path %></p>
